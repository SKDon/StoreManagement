@model StoreManagement.Data.Entities.Navigation

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Navigation</legend>

    <div class="display-label">StoreId</div>
    <div class="display-field">@Model.StoreId</div>

    <div class="display-label">ParentId</div>
    <div class="display-field">@Model.ParentId</div>

    <div class="display-label">Static</div>
    <div class="display-field">@Model.Static</div>

    <div class="display-label">Name</div>
    <div class="display-field">@Model.Name</div>

    <div class="display-label">Title</div>
    <div class="display-field">@Model.Title</div>

    <div class="display-label">Description</div>
    <div class="display-field">@Model.Description</div>

    <div class="display-label">ImagePath</div>
    <div class="display-field">@Model.ImagePath</div>

    <div class="display-label">Ordering</div>
    <div class="display-field">@Model.Ordering</div>

    <div class="display-label">ImageState</div>
    <div class="display-field">@Model.ImageState</div>

    <div class="display-label">Lang</div>
    <div class="display-field">@Model.Lang</div>

    <div class="display-label">Link</div>
    <div class="display-field">@Model.Link</div>

    <div class="display-label">LinkState</div>
    <div class="display-field">@Model.LinkState</div>

    <div class="display-label">CreatedDate</div>
    <div class="display-field">@String.Format("{0:g}", Model.CreatedDate)</div>

    <div class="display-label">PageMetaKeys</div>
    <div class="display-field">@Model.PageMetaKeys</div>

    <div class="display-label">IsController</div>
    <div class="display-field">@Model.IsController</div>

    <div class="display-label">IsAction</div>
    <div class="display-field">@Model.IsAction</div>

    <div class="display-label">IsMainMenu</div>
    <div class="display-field">@Model.IsMainMenu</div>

    <div class="display-label">State</div>
    <div class="display-field">@Model.State</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


