@model StoreManagement.Data.Entities.EmailList

@{
    ViewBag.Title = "Details";
}




<div class="col-md-12">
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="text-muted bootstrap-admin-box-title">Labels</div>
            </div>
            <div class="bootstrap-admin-panel-content">
                <h2>Label Management</h2>

             
                
<h2>Details</h2>

<fieldset>
    <legend>EmailList</legend>

    <div class="display-label">StoreId</div>
    <div class="display-field">@Model.StoreId</div>

    <div class="display-label">CreatedDate</div>
    <div class="display-field">@String.Format("{0:g}", Model.CreatedDate)</div>

    <div class="display-label">UpdatedDate</div>
    <div class="display-field">@String.Format("{0:g}", Model.UpdatedDate)</div>

    <div class="display-label">State</div>
    <div class="display-field">@Model.State</div>

    <div class="display-label">Ordering</div>
    <div class="display-field">@Model.Ordering</div>

    <div class="display-label">Email</div>
    <div class="display-field">@Model.Email</div>

    <div class="display-label">FirstName</div>
    <div class="display-field">@Model.Name</div>

 
</fieldset>
<p>
    @Html.ActionLink("Edit", "SaveOrEdit", new { id=Model.Id },new{@class="btn btn-success btn-lg btn-block"})
                        @Html.ActionLink("Back to List", "Index",null,new{@class="btn btn-default btn-lg btn-block"})
</p>


            </div>
        </div>
    </div>



</div>

