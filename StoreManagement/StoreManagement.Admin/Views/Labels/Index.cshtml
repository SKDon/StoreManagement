@model IEnumerable<StoreManagement.Data.Entities.Label>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>StoreId
        </th>
        <th>CategoryId
        </th>
        <th>Name
        </th>
        <th>ItemType
        </th>
        <th>UpdatedDate
        </th>
        <th>CreatedDate
        </th>
        <th>Ordering
        </th>
        <th>State
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
            <td>
                @item.StoreId
            </td>
            <td>
                @item.CategoryId
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.ItemType
            </td>
            <td>
                @String.Format("{0:g}", item.UpdatedDate)
            </td>
            <td>
                @String.Format("{0:g}", item.CreatedDate)
            </td>
            <td>
                @item.Ordering
            </td>
            <td>
                @item.State
            </td>
        </tr>
    }

</table>


